{
    "name": "kaizen/aws_agent",
    "description": "Aws wrapper for Kaizen",
    "type": "library",
    "require": {
        "php": "^7.4 || ^8.0",
        "aws/aws-sdk-php": "^3.337"
    },
    "autoload": {
        "psr-4": {
            "Kaizen\\AwsAgent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kaizen\\AwsAgent\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.10"
    }
}
